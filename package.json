{
  "name": "vueventjs",
  "version": "1.0.6",
  "repository": "git@github.com:ShailenNaidoo/vuevent.git",
  "author": "shailen <shailen@lumkani.com>",
  "license": "MIT",
  "main": "dist/vuevent.ssr.js",
  "browser": "dist/vuevent.esm.js",
  "module": "dist/vuevent.esm.js",
  "unpkg": "dist/vuevent.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "mock-app": "vue-cli-service serve mock-app/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-replace": "^2.3.1",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "codecov": "^3.6.5",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "fs-extra": "^8.1.0",
    "minify": "^4.1.3",
    "minimist": "^1.2.5",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.4.0",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "babel-jest": "^25.2.6"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npx eslint --ext .js . && npx jest"
    }
  }
}
